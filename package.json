{
  "name": "mfc-supabase",
  "version": "1.0.0",
  "description": "Malappuram FC Website with Supabase + Next.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "setup": "echo 'Setting up MFC Supabase project...' && npm install && echo 'Setup complete! Check SUPABASE_SETUP_GUIDE.md for next steps.'",
    "check:env": "node -e \"console.log('Checking environment...'); console.log('NEXT_PUBLIC_SUPABASE_URL:', process.env.NEXT_PUBLIC_SUPABASE_URL ? 'Set' : 'Missing'); console.log('NEXT_PUBLIC_SUPABASE_ANON_KEY:', process.env.NEXT_PUBLIC_SUPABASE_ANON_KEY ? 'Set' : 'Missing');\""
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@types/aos": "^3.0.7",
    "@types/react-toastify": "^4.0.2",
    "aos": "^2.3.4",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "keywords": [
    "football",
    "malappuram",
    "supabase",
    "nextjs",
    "typescript",
    "admin",
    "secure"
  ],
  "author": "MFC Team",
  "license": "MIT"
}
